<details>
<summary><b>Previous Games 🏑</b></summary>

<table>
    <tr>
        <th>Game No</th>
        <th>Avatar</th>
        <th>User Name</th>
        <th>Issue No</th>
        <th>What Gussed?</th>
        <th>Actual No.</th>
    </tr>
    <% guesses.map(({ username, issue_number, guessed_number, actual_number }, index)=> { %><tr>
        <td align="center"><b><i> <%= guesses.length - index %> </i></b></td>
        <td> <img src="https://avatars.githubusercontent.com/<%= username %>?s=24" width="24px" height="24px" /> </td>
        <td><b><i><a href="https://github.com/<%= username %>"> <%= username %> </a></i></b></td>
        <td align="center"><b><i> <%= issue_number %> </i></b></td>
        <td align="center"><b><i> <%= guessed_number %> </i></b></td>
        <td align="center"><b><i> <%= actual_number %> </i></b></td>
    </tr>
    <% }); %>
</table>

</details>